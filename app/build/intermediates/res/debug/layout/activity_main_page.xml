<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zbar.lib.Main_Page">

    <TextView
        android:id = "@+id/main_page_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/main_page_tip" />

    <EditText
        android:id = "@+id/room_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main_page_tip"/>

    <Button
        android:text="@string/main_page_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/room_number"
        android:onClick="jump_to_camera" />

</RelativeLayout>
